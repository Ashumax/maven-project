pipeline {
     agent any
     stages {
         stage('clone my code'){
           git 'https://github.com/ashumax/maven-project'
           }
          stage ('compile my code') {
          
              steps {
                  withMaven(maven : 'LocalMaven') {
                      sh 'mvn compile'
                      
                      }
                      }
                      }
                      }
                      }
